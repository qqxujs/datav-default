  "keywords": [
    "datav",
    "vue",
    "Cube",
    "screen",
    "lowCode",
    "datav"
  ],
  "license": "MIT",
  "mail": "861236969@qq.com",
  "github": "https://github.com/cubexjs",
  "main": "index.cjs",
  "module": "dist/datav-default.mjs",
  "unpkg": "dist/datav-default.iife.js",
  "jsdelivr": "dist/datav-default.iife.js",
  "types": "dist/datav-default.d.ts",
  "exports": {
    ".": {
      "types": "./dist/datav-default.d.ts",
      "node": {
        "import": {
          "production": "./dist/datav-default.prod.cjs",
          "development": "./dist/datav-default.mjs",
          "default": "./dist/datav-default.mjs"
        },
        "require": {
          "production": "./dist/datav-default.prod.cjs",
          "development": "./dist/datav-default.cjs",
          "default": "./index.cjs"
        }
      },
      "import": "./dist/datav-default.mjs",
      "require": "./index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "start": "vite && npm run docs",
    "dev": "vite",
    "build": "tsc && vue-tsc -b && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "docs": "vitepress dev ./docs",
    "docs:build": "vitepress build ./docs",
    "docs:preview": "vitepress preview ./docs"
  },
  "files": [
    "dist/*.js",
    "dist/*.mjs",
    "dist/*.cjs",
    "dist/datav-default.d.ts",
    "index.cjs",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vitepress": "^1.6.4",
    "vue": "^3.5.22",
    "vue-tsc": "^3.1.0"
  }
}
